<div class="container">
	<div class="row">
		<div class="col-xs-3"></div>
		<div class="col-xs-6">
			<table>
				<thead>
					<tr>
						<th>メッセージ相手</th>
						<th>メッセージ内容naiyou</th>
					</tr>
				</thead>
				<tbody>
					<% @another_entries.each do |entry| %>
					<tr>
						<td><%= link_to entry.user.name, user_path(entry.user) %></td>
						<td><%= link_to room_path(entry.room) do %>
							  <%= Message.find_by(id: entry.room.message_ids.last)&.body %>
							<% end %>
						</td>
					</tr>
				</tbody>
			</table>
		</div>
		<div class="col-xs-3"></div>
	</div>
</div>